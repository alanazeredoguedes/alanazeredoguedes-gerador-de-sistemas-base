{% extends "@ApplicationProjectContent/base/base_web2.html.twig" %}

{% block title %}
    list
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
{% endblock %}

{% block javascriptsFooter %}
    {{ parent() }}
{% endblock %}



{% block body %}
    <div class="wrapper d-flex flex-column flex-row-fluid mt-5 mt-lg-10" id="kt_wrapper">
        <div class="post" id="kt_post">

                <div class="d-flex flex-wrap flex-stack mb-6">
                    <h3 class="fw-bold my-2">
                        Módulos Cadastrados
                        <span class="fs-6 text-gray-400 fw-semibold ms-1">
                            35+ Módulos
                        </span>
                    </h3>
                </div>


                <div class="row g-6 g-xl-9 mb-6 mb-xl-9">

                    {% for i in 0..30 %}

                    <div class="col-md-6 col-lg-4 col-xl-3">

                        <div class="card h-100">

                            <div class="card-body d-flex justify-content-center text-center flex-column p-8">

                                <a href="{{ path('web_documento_list') }}" class="text-gray-800 text-hover-primary d-flex flex-column">

                                    <div class="symbol symbol-75px mb-5">
                                        <img src="{{ asset('bundles/applicationprojectcontent/images/home/folder-document.svg') }}" class="theme-light-show" alt="">
                                    </div>

                                    <div class="fs-5 fw-bold mb-2">Módulo {{ i }}</div>

                                </a>

                                <div class="fs-7 fw-semibold text-gray-400">7 files</div>

                            </div>

                        </div>

                    </div>

                    {% endfor %}

                </div>






        </div>
    </div>
{% endblock %}




