<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>{% block title %}{% endblock %}</title>
    <link rel="icon" href="">
    {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}

{#        <link href="assets/plugins/custom/datatables/datatables.bundle.css" rel="stylesheet" type="text/css" />#}
        <link href="{{ asset('bundles/applicationprojectcontent/plugins/datatables/datatables.bundle.css') }}" rel="stylesheet" type="text/css" />
        <link href="{{ asset('bundles/applicationprojectcontent/css/home/plugins.bundle.css') }}" rel="stylesheet" type="text/css" />
        <link href="{{ asset('bundles/applicationprojectcontent/css/home/style.bundle.css') }}" rel="stylesheet" type="text/css" />

    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}

</head>
<body data-kt-name="metronic" id="kt_body" class="header-fixed header-tablet-and-mobile-fixed aside-enabled sidebar-enabled">





<div class="d-flex flex-column flex-root">
    <div class="page d-flex flex-column flex-column-fluid">

        {{ include('@ApplicationProjectContent/base/menu.html.twig') }}

        <div id="kt_content_container" class="d-flex flex-column-fluid align-items-stretch container-xxl">

            {% block body %}



            {% endblock %}

        </div>
    </div>
</div>






<div id="kt_scrolltop" class="scrolltop" data-kt-scrolltop="true">
        <!--begin::Svg Icon | path: icons/duotune/arrows/arr066.svg-->
