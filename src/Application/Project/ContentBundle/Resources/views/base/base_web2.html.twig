    </div>

{% block javascriptsFooter %}

    <script src="{{ asset('bundles/applicationprojectcontent/js/home/plugins.bundle.js') }}"></script>
    <script src="{{ asset('bundles/applicationprojectcontent/js/home/scripts.bundle.js') }}"></script>
    <script src="{{ asset('bundles/applicationprojectcontent/plugins/datatables/datatables.bundle.js') }}"></script>

<script>
    $(document).ready( function () {
        $('#kt_ecommerce_products_table').DataTable();
    } );
</script>
{% endblock %}

</body>
</html>